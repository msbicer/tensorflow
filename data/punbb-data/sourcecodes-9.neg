aders."\r\n\r\n".$message."\r\n");	fwrite($socket, '.'."\r\n");	server_parse($socket, '250');	fwrite($socket, 'QUIT'."\r\n");	fclose($socket);	return true;}define('FORUM_EMAIL_FUNCTIONS_LOADED', 1);
