			break;		case 'subscriptions_properties':			doSubscriptions();			break;		case 'subscription_properties':			doSubscription();			break;		case 'cluster_properties':			doCluster();			break;		case 'clusters_properties':		default:			doClusters();			break;	}		$misc->printFooter();?>
